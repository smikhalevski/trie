{
  "name": "@smikhalevski/trie",
  "version": "1.0.0",
  "description": "The compact trie data structure.",
  "main": "./lib/index-cjs.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "set -e; tsc; npm run rollup; npm run terser",
    "rollup": "rollup --input ./lib/index.js --file ./lib/index-cjs.js --format cjs --plugin @rollup/plugin-node-resolve",
    "terser": "terser --compress --mangle toplevel --output ./lib/index-cjs.js -- ./lib/index-cjs.js",
    "clean": "rm -rf ./lib",
    "test": "jest --detectOpenHandles",
    "perf": "[ -d ./lib ] || npm run build; node --expose-gc ./src/test/perf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/trie.git"
  },
  "keywords": [
    "trie",
    "map",
    "search",
    "string"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/trie/issues"
  },
  "homepage": "https://github.com/smikhalevski/trie#readme",
  "jest": {
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@smikhalevski/perf-test": "^1.0.0",
    "@types/jest": "^26.0.23",
    "jest": "^27.0.2",
    "rollup": "^2.50.5",
    "terser": "^5.7.0",
    "ts-jest": "^27.0.2",
    "typescript": "4.2.4"
  }
}
